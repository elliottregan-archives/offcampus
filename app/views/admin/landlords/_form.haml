= form_for(@landlord) do |f|
  - if @landlord.errors.any?
    #error_explanation
      %h2
        = pluralize(@landlord.errors.count, "error")
        prohibited this landlord from being saved:
      %ul
        - @landlord.errors.full_messages.each do |msg|
          %li= msg
  %fieldset
    = f.label :name
    = f.text_field :name
  %fieldset
    = f.label :phone
    = f.text_field :phone
  %fieldset
    = f.label :email
    = f.text_field :email
  .actions
    = f.submit